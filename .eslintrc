{
  "extends": ["plugin:prettier/recommended", "next", "next/core-web-vitals"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "singleQuote": true
      }
    ],
    "react/react-in-jsx-scope": "off",
    "react/no-unescaped-entities": "off",
    "react/prop-types": [0, { "ignore": ["children"] }],
    "@next/next/no-img-element": 0
  }
}
